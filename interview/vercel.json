{
    "version": 2,
    "builds": [
      {
        "src": "src/app.tsx", 
        "use": "@vercel/static-build", 
        "config": {
          "root": "build" 
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  